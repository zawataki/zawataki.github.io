{
    "editor.rulers": [
        80
    ],
    "html.format.indentInnerHtml": false,
    "html.format.wrapLineLength": 80,
    "html.format.wrapAttributes": "auto",
    "html.format.endWithNewline": true,
    "[javascript]": {
        "editor.tabSize": 4,
        "editor.insertSpaces": true
    },
    "files.eol": "\n",
    "cSpell.ignoreRegExpList": [
        "[０-９Ａ-Ｚａ-ｚぁ-んァ-ヶ亜-熙纊-黑]+"
    ],
}
